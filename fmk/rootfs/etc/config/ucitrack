config network
#	option init network
#	list affects dhcp
#	list affects logcfg
#	list affects radvd
	option exec '/sbin/reboot_script' 

config wireless
	#list affects network
	option exec /sbin/wifi

config firewall
	option init firewall
	list affects luci-splash
	list affects qos
	list affects upnpd
	list affects uhttpd

config olsr
	option init olsrd

config dhcp
	option init dnsmasq
	list affects  limit

config limit
	option init limit

config portal
	option init portal

config dropbear
	option init dropbear

config httpd
	option init httpd

config fstab
	option init fstab

config qos
	option init qos
	
config upnpd
	option init miniupnpd_init

config ntpclient
	option init ntpclient

config samba
	option init samba

config tinyproxy
	option init tinyproxy

config wanctl
	option init wanctl

config services 
	option init services 

config remote 
	option init tr069 

config igmp 
	option init igmp

config routes  
	option init routes

config ddns  
	option exec '/sbin/ddns_scripts.sh' 

config devinfo
	option exec '/sbin/devinfo.sh'

#config logcfg  
config system 
	option init loginit

config port
	option init portinit

config voip
	option init voip_init.sh

config ftpd 
	option init proftpd 

config uhttpd
    option init uhttpd
    option exec '/etc/init.d/firewall reload DMZ'
